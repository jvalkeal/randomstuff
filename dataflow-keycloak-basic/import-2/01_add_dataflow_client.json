{
  "realm": "dataflow",
  "clients": [
    {
      "clientId": "system:dataflow",
      "enabled": true,
      "secret": "secret",
      "redirectUris": [
        "http://localhost:9393/*"
      ],
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": true,
      "publicClient": false,
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "60"
      }
    }
  ]
}
